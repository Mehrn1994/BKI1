 set peer 10.30.42.200
 set transform-set INTR1 
 match address INTR1ACL
crypto map BKI-INT-Tunnel 9 ipsec-isakmp 
 set peer 10.30.42.201
 set transform-set INTR2 
 match address INTR2ACL
!
!
